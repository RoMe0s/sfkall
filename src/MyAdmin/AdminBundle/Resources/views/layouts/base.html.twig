    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% block stylesheets %}
        {% include '@AdminBundle/Resources/views/partials/styles.html.twig' %}
    {% endblock %}
    <link href="{{ asset('bundles/admin/css/custom.css') }}" rel="stylesheet">
</head>

<body>
